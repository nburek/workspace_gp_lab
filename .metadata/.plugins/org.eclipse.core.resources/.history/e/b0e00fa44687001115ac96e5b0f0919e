class Dial: Widget{

	~Dial(){};
	void render(){}

	//universal interface to run a function
	//TO DO: enforce the function table, make this non-virtual
	//Packet runFunction(u8 function, const Packet& pack) = 0;
	void move(u16 x_s, u16 y_s){
		x = x_s;
		y = y_s;
	}



}

