

#include <stdio.h>
#include <math.h>
#include "platform.h"
#include "xbasic_types.h"
#include "xuartlite_l.h"
#include "xparameters.h"
#include "widget.h"
#include "widget_table.h"
#include "packet.h"

void print(char *str);

void testy(int* a, int* b){

	int t = *a;
	*a = b;
	*b = t;
}

int main()
{
    init_platform();

    struct widget w;

    w.move(&w, 0, 0);

    print("Hello World\n\r");

    cleanup_platform();

    return 0;
}
