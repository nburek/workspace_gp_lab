

#include <stdio.h>
#include <math.h>
#include "platform.h"
#include "xbasic_types.h"
#include "xuartlite_l.h"
#include "xparameters.h"
#include "widget.h"
#include "widget_table.h"
#include "packet.h"

void print(char *str);

int testy(int* a, int* b){

	int t = *a;
	*a = *b;
	*b = t;

	return 9;
}

int main()
{
    init_platform();

    struct widget w;

    int d =1;
    int s = 4;

    w.move(&w, testy(d, s), 0);

    print("Hello World\n\r");

    cleanup_platform();

    return 0;
}
