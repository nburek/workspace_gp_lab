#include "RoundMeter.cpp"
#include "Graphics.h"

RoundMeter::RoundMeter(int x, int y) : Widget(x,y)
{

}

void RoundMeter::draw()
{
	drawCircle(this->x,this->y,100,BLACK,1);
	drawCircle(this->x,this->y,90,WHITE,1);
}
